{
    "headers": [
      {
        "source": "/(.*)\\.(jpg|jpeg|png|gif|webp|avif|css|js|woff2|woff|ttf|svg|eot|otf)$",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*)\\.(html|htm)$",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }
  